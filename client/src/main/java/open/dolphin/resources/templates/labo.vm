#else
<TD NOWRAP>${model.getOrderName()}($stampName)</TD>
<TD NOWRAP COLSPAN="2" ALIGN="RIGHT">${model.getClassCode()}</TD></TR>
#end
#else
<TD NOWRAP>${model.getOrderName()}</TD>
<TD NOWRAP COLSPAN="2" ALIGN="RIGHT">${model.getClassCode()}</TD></TR>
#end
	</TR>
	<TR>
		<TD>・${model.getItemNames()}</TD>
	</TR>
#if ($model.getMemo())
	<TR>
		<TD COLSPAN="3">$!{model.getMemo()}</TD>
	</TR>
#end
</TABLE>	
